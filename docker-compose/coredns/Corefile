.:53 {
    bind 0.0.0.0
    cache 300
    hosts /etc/coredns/core.hosts {
    	fallthrough
	}
    proxy . 114.114.114.114:53 {
        protocol dns
       	fail_timeout 2s
	    max_fails 3
    }
    errors
    log . "{remote} - [{when}] {>id} {type} {class} {name} {proto} {size} {>do} {>bufsize} {rcode} {>rflags} {rsize} {duration}"
}
